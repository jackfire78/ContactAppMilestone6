<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jackf.contactapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".global"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewContact" />
        <activity android:name=".contactInfoPage" />
        <activity android:name=".contactInfoBusiness" />
        <activity android:name=".NewBusinessContact" />
        <activity android:name=".Search"></activity>
    </application>

</manifest>